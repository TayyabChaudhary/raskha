{
  "expo": {
    "name": "raskha",
    "slug": "raskha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-icon.png",
    "schema": "raskha",
    "backgroundColor": "#9F1F72",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "This app requires access to your contacts to select emergency contacts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-icon.png",
        "backgroundColor": "#9F1F72"
      },
      "permissions": [
        "READ_CONTACTS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#9F1F72"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "backgroundColor": "#9F1F72",
      "resizeMode": "contain",
      "image": "./assets/images/logo-icon.png"
    }
  }
}